{
  "name": "@codeforafrica/hurumap-wp-plugin",
  "version": "0.1.0",
  "main": "build/index.js",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.0.0",
    "@wordpress/scripts": "^5.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "lint": "yarn eslint --fix  './src'",
    "start": "concurrently \"yarn start:data-page\"  \"yarn start:blocks\"",
    "start:blocks": "wp-scripts start ./src/FlourishBlock/index.js ./src/HURUmapBlock/index.js ./src/FeaturedData/index.js ./src/CardBlock/index.js --output-path=build/blocks",
    "start:data-page": "wp-scripts start ./src/index.js --output-path=build/data-page",
    "build": "yarn build:data-page && yarn build:blocks",
    "build:data-page": "wp-scripts build ./src/index.js --output-path=build/data-page",
    "build:blocks": "wp-scripts build ./src/FlourishBlock/index.js ./src/HURUmapBlock/index.js ./src/FeaturedData/index.js ./src/CardBlock/index.js --output-path=build/blocks",
    "build:embed": "wp-scripts build ./src/embed/index.js -o ../../themes/hurumap/assets/build/embed.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@codeforafrica/hurumap-ui": "^0.2.0-alpha.6",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "@reach/router": "^1.2.1",
    "@wordpress/blocks": "^6.7.0",
    "@wordpress/components": "^8.3.2",
    "@wordpress/data": "^4.9.2",
    "@wordpress/editor": "^9.7.0",
    "@wordpress/element": "^2.8.0",
    "@wordpress/i18n": "^3.6.0",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "formik": "^1.5.8",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-ace": "^7.0.4",
    "react-dom": "^16.10.1",
    "react-dropzone": "^10.1.10",
    "react-resize-detector": "^4.2.1",
    "react-select": "^3.0.5",
    "react-transition-group": "^4.3.0",
    "shortid": "^2.2.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "yarn lint",
      "git add"
    ]
  }
}
